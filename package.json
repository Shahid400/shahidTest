{
  "name": "resturaunt_info_system",
  "version": "1.0.0",
  "description": "Resturaunt info System",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "author": "Shahid Awan",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/opossum": "^6.2.0",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@types/node": "^16.9.6",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "geolib": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0",
    "opossum": "^6.2.1",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.11.2"
  }
}
